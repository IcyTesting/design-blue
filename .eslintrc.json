{
	"env" : {
		"es6" : true,
		"browser" : true,
		"node" : true,
		"jasmine" : true,
		"jquery" : true
	},
	"extends" : "eslint:recommended",
	"parserOptions" : {
		"sourceType" : "module"
	},
	"rules" : {
		"indent" : [
			"error",
			"tab"
		],
		"linebreak-style" : [
			"error",
			"unix"
		],
		"quotes" : [
			"error",
			"single"
		],
		"semi" : [
			"error",
			"always"
		]
	}
}