Options -Indexes

<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /www.dbs-web.dev
RewriteRule ^index\.php$ - [L]

RewriteCond %{REQUEST_FILENAME} !-f
#RewriteCond %{REQUEST_FILENAME} !-d

#RewriteCond %{SCRIPT_FILENAME} !-d
#RewriteCond %{SCRIPT_FILENAME} !-f

RewriteRule . /index.php [L]
</IfModule>

<IfModule mod_rewrite.c>
RewriteEngine On
RewriteCond %{REQUEST_URI} ^(/app/|/iCantSleep/|/vendor/|/view/).*$
RewriteRule .*$ /index.php
</IfModule>

<FilesMatch "(.json|.lock|.htaccess)(/|$)">
Order allow,deny
Deny from All
Satisfy All
</FilesMatch>

